        }
    </style>
    <script>
      document.addEventListener('DOMContentLoaded', () => {
          const sliderWrapper = document.querySelector('.About-container');
          const leftArrow = document.querySelector('.custom-arrow-left');
          const rightArrow = document.querySelector('.custom-arrow-right');
          const items = document.querySelectorAll('.about-column');
          const totalItems = items.length;
          let itemWidth = items[0].offsetWidth + 20; // Width + gap
          let currentIndex = 0;
          let isScrolling = false;
      
          // Update slider position
          function updateSlider() {
              if (window.innerWidth <= 768) {
                  const scrollPosition = currentIndex * itemWidth;
                  sliderWrapper.scrollTo({
                      left: scrollPosition,
                      behavior: 'smooth'
                  });
              } else {
                  sliderWrapper.scrollTo({ left: 0 });
                  sliderWrapper.style.scrollSnapType = 'none';
              }
          }
      
          // Update item width on resize
          function updateItemWidth() {
              itemWidth = items[0].offsetWidth + 20;
              updateSlider();
          }
      
          // Left arrow click
          leftArrow.addEventListener('click', () => {
              if (window.innerWidth <= 768 && currentIndex > 0 && !isScrolling) {
                  isScrolling = true;
                  currentIndex--;
                  updateSlider();
              }
          });
      
          // Right arrow click
          rightArrow.addEventListener('click', () => {
              if (window.innerWidth <= 768 && currentIndex < totalItems - 1 && !isScrolling) {
                  isScrolling = true;
                  currentIndex++;
                  updateSlider();
              }
          });
      
          // Sync manual scroll with currentIndex
          sliderWrapper.addEventListener('scroll', () => {
              if (window.innerWidth <= 768 && !isScrolling) {
                  const scrollLeft = sliderWrapper.scrollLeft;
                  const newIndex = Math.round(scrollLeft / itemWidth);
                  if (newIndex !== currentIndex) {
                      currentIndex = Math.max(0, Math.min(newIndex, totalItems - 1));
                  }
              }
          });
      
          // Reset isScrolling flag after scroll animation
          sliderWrapper.addEventListener('scrollend', () => {
              isScrolling = false;
          });
      
          // Handle window resize
          window.addEventListener('resize', () => {
              updateItemWidth();
              currentIndex = 0;
              updateSlider();
          });
      
          // Initial setup
          updateItemWidth();
          updateSlider();
      });
    </script>
  {% endcomment %}
